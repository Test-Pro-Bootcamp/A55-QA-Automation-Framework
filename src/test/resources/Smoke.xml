<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Smoke" parallel="methods" thread-count="1">
    <parameter name="BaseUrl" value="https://qa.koel.app/"/>
    <test name="Example Test" preserve-order="false">
        <classes>
            <class name="LoginTests">
                <methods>
                    <include name="loginWithValidCredentials"></include>
                    <include name="updateEmail"></include>
                    <include name="loginWithUpdatedEmail"></include>
                    <include name="updatePassword"></include>
                    <include name="loginWithUpdatedPassword"></include>
                </methods>
            </class>
            <class name="LogoutTests"/>
        </classes>
    </test>
</suite>